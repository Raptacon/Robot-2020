{
    "inputs": {
        "XboxControllers": {
            "drive": {
                "type": "XboxController",
                "channel": 0
            },
            "mech": {
                "type": "XboxController",
                "channel": 1
            }
        }
    },
    "motors": {
        "driveTrain": {
            "rightMotor": {
                "channel": 30,
                "type": "CANTalonFX",
                "inverted": true,
                "currentLimits": {
                    "triggerThresholdCurrent": 60,
                    "triggerThresholdTime": 50,
                    "currentLimit": 40
                }
            },
            "rightFollower": {
                "channel": 31,
                "masterChannel": 30,
                "follower": true,
                "type": "CANTalonFX",
                "inverted": true,
                "currentLimits": {
                    "triggerThresholdCurrent": 60,
                    "triggerThresholdTime": 50,
                    "currentLimit": 40
                }
            },
            "leftMotor": {
                "channel": 20,
                "type": "CANTalonFX",
                "inverted": true,
                "currentLimits": {
                    "triggerThresholdCurrent": 60,
                    "triggerThresholdTime": 50,
                    "currentLimit": 40
                }
            },
            "leftFollower": {
                "channel": 21,
                "masterChannel": 20,
                "follower": true,
                "type": "CANTalonFX",
                "inverted": true,
                "currentLimits": {
                    "triggerThresholdCurrent": 60,
                    "triggerThresholdTime": 50,
                    "currentLimit": 40
                }
            }
        },
        "intake": {
            "intakeMotor": {
                "channel": 50,
                "type": "CANTalonSRX",
                "inverted": false,
                "currentLimits": {
                    "absMax": 10,
                    "absMaxTimeMs": 50,
                    "maxNominal": 6
                }
            }
        },
        "loader": {
            "loaderMotor": {
                "channel": 51,
                "type": "CANTalonSRX",
                "inverted": true,
                "currentLimits": {
                    "absMax": 60,
                    "absMaxTimeMs": 50,
                    "maxNominal": 40
                }
            }
        },
        "shooter": {
            "shooterMotor": {
                "channel": 52,
                "type": "CANSparkMax",
                "inverted": true,
                "motorType": "kBrushless",
                "pid": {
                    "controlType": "kVelocity",
                    "kP": 0.0004,
                    "kI": 0,
                    "kD": 0,
                    "kF": 0.000175,
                    "feedbackDevice": 1,
                    "kPreScale": 1,
                    "coastOnZero": true
                },
                "currentLimits": {
                    "freeLimit": 30,
                    "stallLimit": 30,
                    "stallLimitRPM": 50,
                    "secondaryLimit": 35
                }
            }
        },
        "elevator": {
            "elevatorMotor": {
                "channel": 60,
                "type": "CANTalonSRX",
                "inverted": false,
                "currentLimits": {
                    "absMax": 60,
                    "absMaxTimeMs": 50,
                    "maxNominal": 40
                }
            }
        },
        "winch": {
            "winchMotor": {
                "channel": 40,
                "type": "CANSparkMax",
                "inverted": true,
                "motorType": "kBrushless",
                "sensorPhase": true,
                "currentLimits": {
                    "freeLimit": 30,
                    "stallLimit": 30,
                    "stallLimitRPM": 50,
                    "secondaryLimit": 35
                }
            }
        }
    },
    "sensors": {
        "breaksensors": {
            "sensor1": {
                "type": "RIODigitalIn",
                "channel": 5,
                "description": "Loading sensor"
            },
            "sensor2": {
                "type": "RIODigitalIn",
                "channel": 4,
                "description": "Second bottom sensor"
            },
            "sensor3": {
                "type": "RIODigitalIn",
                "channel": 3,
                "description": "Middle sensor"
            },
            "sensor4": {
                "type": "RIODigitalIn",
                "channel": 2,
                "description": "Second top sensor"
            },
            "sensor5": {
                "type": "RIODigitalIn",
                "channel": 1,
                "description": "Shooting sensor"
            }
        },
        "gyros": {
            "navx": {
                "type": "navx",
                "method": "spi"
            }
        }
    },
    "pneumatics": {
        "compressors": {
            "compressor": {
                "type": "compressor"
            }
        },
        "solenoids": {
            "intakeSolenoid": {
                "type": "doubleSolenoid",
                "default": "kForward",
                "channel": {
                    "forward": 6,
                    "reverse": 7,
                    "pcm": 0
                }
            }
        }
    }
}